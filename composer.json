{
    "name": "tipoff/checkout",
    "description": "Laravel Package for Ecommerce Checkouts",
    "keywords": [
        "tipoff",
        "checkout"
    ],
    "homepage": "https://github.com/tipoff/checkout",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "tipoff/authorization": "^1.0.1",
        "tipoff/locations": "^2.2.1",
        "tipoff/support": "dev-pdbreen/feature/amount-each as 1.5.1"
    },
    "require-dev": {
        "tipoff/test-support": "^1.0.6"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\Checkout\\": "src",
            "Tipoff\\Checkout\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Checkout\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Checkout\\CheckoutServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
